{
    "bind_address": "0.0.0.0",
    "url_base": "/",
    "port": "8282",
    "log_level": "info",
    "debrids": [],
    "qbittorrent": {
        "download_folder": "/mnt/symlinks",
        "refresh_interval": 15,
        "max_downloads": 25
    },
    "repair": {
        "workers": 1,
        "strategy": "per_torrent"
    },
    "webdav": {},
    "rclone": {
        "enabled": true,
        "mount_path": "/mnt/remote",
        "vfs_cache_mode": "minimal",
        "vfs_cache_max_age": "1h",
        "vfs_cache_poll_interval": "1m",
        "vfs_read_chunk_size": "128M",
        "vfs_read_chunk_size_limit": "off",
        "vfs_read_ahead": "128k",
        "buffer_size": "10M",
        "uid": 1000,
        "gid": 1000,
        "umask": "002",
        "attr_timeout": "1s",
        "dir_cache_time": "5m",
        "log_level": "INFO"
    },
    "allowed_file_types": [
