{
  "services": [
    {
      "name": "cinesync",
      "image": "sureshfizzy/cinesync:latest",
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "UTC"
      },
      "internalPort": 5173,
      "addPorts": [
        {
          "containerPort": 8082,
          "hostPort": 8082,
          "tcp": true,
          "udp": false
        }
      ],
      "volumes": [
        {
          "hostPath": "./.env",
          "containerPath": "/app/.env",
          "readOnly": false,
          "shared": false,
          "private": false
        },
        {
          "hostPath": "./db",
          "containerPath": "/app/db",
          "readOnly": false,
          "shared": false,
          "private": false
        },
        {
          "hostPath": "/home",
          "containerPath": "/home",
          "readOnly": false,
          "shared": false,
          "private": false
        },
        {
          "hostPath": "/mnt",
          "containerPath": "/mnt",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}